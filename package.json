{
  "name": "@yasai/piman",
  "description": "Piman is an open-source accessibility UI framework.",
  "author": "Blueplanet, Inc.",
  "version": "3.0.0-alpha.2",
  "private": false,
  "type": "module",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md",
    "src/components",
    "src/assets"
  ],
  "main": "./dist/piman-vue3.umd.js",
  "module": "./dist/piman-vue3.es.js",
  "exports": {
    ".": {
      "import": "./dist/piman-vue3.es.js",
      "require": "./dist/piman-vue3.umd.js"
    },
    "./dist/piman-vue3.css": {
      "import": "./dist/piman-vue3.css",
      "require": "./dist/piman-vue3.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.60.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
